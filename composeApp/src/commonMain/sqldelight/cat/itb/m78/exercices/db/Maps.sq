CREATE TABLE MapsMarkersSave(
    id INTEGER PRIMARY KEY NOT NULL,
    title TEXT NOT NULL,
    y REAL NOT NULL,
    x REAL NOT NULL,
    url TEXT NOT NULL
);

CREATE INDEX maps_index ON MapsMarkersSave (id);

selectAll:
SELECT * FROM MapsMarkersSave;

insert:
INSERT INTO MapsMarkersSave(title, y, x, url) VALUES (?, ?, ?, ?);

findByMarkerId:
SELECT * FROM MapsMarkersSave WHERE id = ?;